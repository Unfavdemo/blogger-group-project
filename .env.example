# Copy this file to .env.local or .env and fill in your Neon/Postgres connection string
# Example Neon connection string format (replace with your real values):
# postgres://<username>:<password>@<host>/<database>

DATABASE_URL="postgres://username:password@your-neon-endpoint/neondb"

# NextAuth secret (optional for auth flows)
NEXTAUTH_SECRET="change-me"

# Example: set to development to enable debug logs
NODE_ENV=development
